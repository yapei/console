{
  "name": "bridge",
  "description": "CoreOS Tectonic Dashboard",
  "repository": "https://github.com/coreos-inc/bridge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "rm -rf ./public/dist/ && webpack --watch",
    "build": "rm -rf ./public/dist && NODE_ENV=production webpack --progress",
    "coverage": "jest --coverage .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --color .",
    "test": "jest",
    "test-gui": "nightwatch --test integration-tests/tests/crud.js",
    "analyze": "NODE_ENV=production webpack --profile --json | awk '{if(NR>7)print}' > public/dist/stats.json && webpack-bundle-analyzer --mode static -r public/dist/report.html public/dist/stats.json"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      ".(ts|tsx|js|jsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js|jsx)$",
    "setupFiles": [
      "./before-tests.js",
      "./__mocks__/localStorage.js"
    ],
    "coverageDirectory": "__coverage__",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ],
    "collectCoverageFrom": [
      "public/*.{js,jsx}",
      "public/{components,module,ui}/**/*.{js,jsx}"
    ]
  },
  "dependencies": {
    "blueimp-md5": "2.x",
    "brace": "^0.10.0",
    "classnames": "2.x",
    "d3": "4.x",
    "file-saver": "1.3.x",
    "font-awesome": "4.7.x",
    "fuzzysearch": "1.0.x",
    "history": "4.x",
    "immutable": "3.8.x",
    "js-yaml": "3.x",
    "lodash": "4.x",
    "moment": "2.x",
    "plotly.js": "1.x",
    "prop-types": "15.x",
    "react": "15.6.x",
    "react-addons-css-transition-group": "15.6.x",
    "react-dom": "15.6.x",
    "react-helmet": "5.x",
    "react-lightweight-tooltip": "0.x",
    "react-modal": "1.x",
    "react-redux": "5.x",
    "react-router-dom": "4.x",
    "react-tagsinput": "3.x",
    "redux": "3.x",
    "redux-form": "6.x",
    "redux-thunk": "2.x",
    "url-search-params-polyfill": "2.x",
    "whatwg-fetch": "2.x"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "@types/immutable": "3.x",
    "@types/jest": "20.x",
    "@types/lodash": "4.x",
    "@types/moment": "2.x",
    "@types/react": "16.x",
    "@types/react-dom": "15.x",
    "@types/react-helmet": "5.x",
    "@types/react-redux": "5.0.7",
    "awesome-typescript-loader": "^3.2.3",
    "chromedriver": "2.x",
    "css-loader": "0.28.x",
    "enzyme": "^2.9.1",
    "eslint": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-react": "7.x",
    "extract-text-webpack-plugin": "3.x",
    "file-loader": "0.11.x",
    "glslify": "5.x",
    "glslify-loader": "1.x",
    "html-webpack-plugin": "2.x",
    "jasmine-core": "2.x",
    "jest-cli": "20.x",
    "nightwatch": "0.9.x",
    "node-sass": "4.x",
    "react-addons-perf": "15.x",
    "react-test-renderer": "15.x",
    "resolve-url-loader": "2.x",
    "sass-loader": "6.x",
    "style-loader": "0.18.x",
    "ts-jest": "20.x",
    "ts-loader": "2.x",
    "typescript": "2.5.1",
    "typescript-eslint-parser": "8.0",
    "uglifyjs-webpack-plugin": "1.0.0-beta.1",
    "webpack": "3.x",
    "webpack-bundle-analyzer": "2.x"
  },
  "engines": {
    "node": "8.x"
  }
}
