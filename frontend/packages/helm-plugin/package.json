{
  "name": "@console/helm-plugin",
  "version": "0.0.0-fixed",
  "description": "Helm plugin contributes helm related features to Developer Console UI.",
  "private": true,
  "scripts": {
    "coverage": "yarn test --coverage --collectCoverageFrom='[\"packages/helm-plugin/src/**\",\"!**/__tests__/**\",\"!packages/helm-plugin/src/test/**\"]'",
    "lint": "yarn --cwd ../.. eslint packages/helm-plugin",
    "test": "yarn --cwd ../.. test packages/helm-plugin"
  },
  "consolePlugin": {
    "entry": "src/plugin.ts",
    "exposedModules": {
      "helmActions": "src/actions/index.ts",
      "helmCatalog": "src/catalog/index.ts",
      "helmComponents": "src/components/index.ts",
      "helmTopology": "src/topology/index.ts",
      "helmTopologySidebar": "src/topology/sidebar/index.ts",
      "helmUtils": "src/utils/index.ts",
      "helmProviders": "src/providers/index.ts"
    }
  }
}
