{
  "name": "@console/knative-plugin",
  "version": "0.0.0-fixed",
  "description": "Knative - Kubernetes-based platform to build, deploy, and manage modern serverless workloads",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "coverage": "yarn test --coverage --collectCoverageFrom='[\"packages/knative-plugin/src/**\",\"!**/__tests__/**\"]'",
    "lint": "yarn --cwd ../.. eslint packages/knative-plugin",
    "test": "yarn --cwd ../.. test packages/knative-plugin"
  },
  "dependencies": {
    "@console/plugin-sdk": "0.0.0-fixed",
    "@console/topology": "0.0.0-fixed"
  },
  "consolePlugin": {
    "entry": "src/plugin.tsx",
    "exposedModules": {
      "actions": "src/actions/providers.ts",
      "icons": "src/utils/icons.tsx",
      "catalog": "src/catalog/index.ts",
      "yamlTemplates": "src/yaml-templates.ts",
      "revisionsComponent": "src/components/revisions/index.ts",
      "routesComponent": "src/components/routes/index.ts",
      "servicesComponent": "src/components/services/index.ts",
      "overviewComponent": "src/components/overview/index.ts",
      "pubSubComponent": "src/components/pub-sub/index.ts",
      "addComponent": "src/components/add/index.ts",
      "knativeComponent": "src/components/index.ts",
      "eventingComponent": "src/components/eventing/index.ts",
      "knatifyComponent": "src/components/knatify/index.ts",
      "knativeTopologySidebar": "src/topology/sidebar/index.ts",
      "topology": "src/topology/index.ts",
      "eventingContext": "src/topology/eventing-context.ts",
      "functionsComponent": "src/components/functions/index.ts"
    }
  }
}
